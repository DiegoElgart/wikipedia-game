{
  "name": "wikipedia-game-server",
  "version": "0.1.0",
  "description": "Typescript + Node.js REST server application",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "debug": "npm run build && npm run watch-debug",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "env-yaml": "0.1.2",
    "express": "4.17.1",
    "errorhandler": "1.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/errorhandler": "1.5.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "nodemon": "^2.0.7",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
